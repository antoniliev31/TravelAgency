@model List<HotelAllViewModel>
@{
    ViewData["Title"] = "Моите обяви";
}

<h2 class="text-center">@ViewData["Title"]</h2>

<hr/>

@if (!Model.Any())
{
    <h2 class="text-center">Все още нищо не сте публикували!</h2>
}

<div class="row mt-0 mb-0">
    @foreach (var hotel in Model)
    {
        <div class="col-md-4">
            <partial name="_HotelPartial" model="@hotel" />
        </div>
        
    }
</div>
